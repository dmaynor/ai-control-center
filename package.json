{
  "name": "ai-control-center",
  "displayName": "AI Control Center",
  "description": "Unified dashboard to manage all LLM, AI, and model integrations in VSCode.",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": [
    "Other",
    "AI",
    "Productivity"
  ],
  "activationEvents": [
    "onCommand:aiControlCenter.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiControlCenter.open",
        "title": "Open AI Control Center"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./ && webpack --mode production",
    "watch": "webpack --watch --mode development",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
